{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "数据工厂名称",
			"defaultValue": "leonzhao-df-testing"
		},
		"AzureSql_ADF_password": {
			"type": "secureString",
			"metadata": "“AzureSql_ADF”的“password”的安全字符串"
		},
		"Blob_Testing_connectionString": {
			"type": "secureString",
			"metadata": "“Blob_Testing”的“connectionString”的安全字符串"
		},
		"Sftp_password": {
			"type": "secureString",
			"metadata": "“Sftp”的“password”的安全字符串"
		},
		"AzureSql_ADF_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "leonzhaotesting.database.windows.net"
		},
		"AzureSql_ADF_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ADF"
		},
		"AzureSql_ADF_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "leonadmin"
		},
		"Sftp_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "leontestingstorage.blob.core.windows.net"
		},
		"Sftp_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "leontestingstorage.leon"
		},
		"containerUri": {
			"type": "string",
			"metadata": "包含链接 ARM 模板的存储帐户的 URI"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "包含链接 ARM 模板的存储帐户的 SAS 令牌"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "leonzhao-df-testing_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSql_ADF_password": {
						"value": "[parameters('AzureSql_ADF_password')]"
					},
					"Blob_Testing_connectionString": {
						"value": "[parameters('Blob_Testing_connectionString')]"
					},
					"Sftp_password": {
						"value": "[parameters('Sftp_password')]"
					},
					"AzureSql_ADF_properties_typeProperties_server": {
						"value": "[parameters('AzureSql_ADF_properties_typeProperties_server')]"
					},
					"AzureSql_ADF_properties_typeProperties_database": {
						"value": "[parameters('AzureSql_ADF_properties_typeProperties_database')]"
					},
					"AzureSql_ADF_properties_typeProperties_userName": {
						"value": "[parameters('AzureSql_ADF_properties_typeProperties_userName')]"
					},
					"Sftp_properties_typeProperties_host": {
						"value": "[parameters('Sftp_properties_typeProperties_host')]"
					},
					"Sftp_properties_typeProperties_userName": {
						"value": "[parameters('Sftp_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}